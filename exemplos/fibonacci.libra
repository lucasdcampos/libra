// Função para listar a sequência de Fibonacci
funcao fibonacci(n: Int)
    var a = 0
    var b = 1
    var contador = 0
    var temp = 0
    enquanto contador < n faca
        exibir(a)
        temp = a
        a = b
        b = temp + b
        contador = contador+1
    fim
fim

// Mostra os 10 primeiros números da sequência de Fibonacci
fibonacci(10)